{"ast":null,"code":"// @ is an alias to /src\nlet {\n  saveTask,\n  getTasks\n} = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    deleteT(id) {\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAeA;AACA,IAAI;EAACA,QAAQ;EAAEC;AAAQ,IAAIC,OAAO,CAAC,yBAAyB;AAC5D,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIM,QAAQ,CAACZ,QAAQ,EAAE;MAC/B,IAAIa,QAAO,GAAIP,KAAK,CAACQ,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACJ,EAAC,IAAKA,EAAE,CAAE;MACvDL,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACV,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ;MACrDJ,QAAQ,CAACb,QAAQ,CAACO,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIM,QAAQ,CAACZ,QAAQ,EAAC;IACjC,CAAC;IACDiB,OAAO,CAACN,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIM,QAAQ,CAACZ,QAAQ,EAAE;MAC/BM,KAAI,GAAIA,KAAK,CAACY,MAAM,CAAEH,IAAI,IAAKA,IAAI,CAACJ,EAAC,KAAMA,EAAE;MAC7CC,QAAQ,CAACb,QAAQ,CAACO,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIM,QAAQ,CAACZ,QAAQ,EAAC;IACjC,CAAC;IACDmB,KAAK,CAACR,EAAE,EAAE;MACR,IAAI,CAACJ,MAAK,GAAI,IAAI;IACpB;EACF,CAAC;EACDa,OAAO,GAAG;IACR,IAAI,CAACd,KAAI,GAAIM,QAAQ,CAACZ,QAAQ,EAAC;EACjC;AACF","names":["saveTask","getTasks","require","TaskTab","name","components","data","tasks","isEdit","currentTask","methods","updateT","id","tasksObj","objIndex","findIndex","task","completed","deleteT","filter","editT","mounted"],"sourceRoot":"","sources":["/Users/vineetdhiman/Desktop/projects/vue/vue-todo/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Task list</h1>\n\n    <div v-if=\"!isEdit\">\n      <div v-for=\"task in tasks\" :key=\"task.title\">\n        <TaskTab :completed=\"task.completed\" :id=\"task.id\" :title=\"task.title\"\n        @updateTask=\"updateT\" @deleteTask=\"deleteT\" @editTask=\"editT\"/>\n      </div>\n    </div>\n    \n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nlet {saveTask, getTasks} = require('../js/commonFunction.js')\nimport TaskTab from '../components/TaskTab.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab,\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    }\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex((task => task.id == id));\n      tasks[objIndex].completed = !tasks[objIndex].completed\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    deleteT(id) {\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter((task) => task.id !== id)\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks()\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}