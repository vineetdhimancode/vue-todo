{"ast":null,"code":"// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nimport TaskForm from '../components/TaskForm.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab,\n    TaskForm\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    deleteT(id) {\n      console.log(id);\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    editT(id) {\n      this.isEdit = true;\n      let tasks = tasksObj.getTasks();\n      this.currentTask = tasks.filter(task => task.id == id)[0];\n      console.log(this.currentTask);\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  },\n  updated() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,IAAIA,QAAO,GAAIC,OAAO,CAAC,yBAAyB;AAChD,OAAOC,OAAM,MAAO,2BAA0B;AAC9C,OAAOC,QAAO,MAAO,4BAA2B;AAEhD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH,OAAO;IACPC;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIP,QAAQ,CAACa,QAAQ,EAAE;MAC/B,IAAIC,QAAO,GAAIP,KAAK,CAACQ,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACJ,EAAC,IAAKA,EAAE,CAAE;MACvDL,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACV,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ;MACrDjB,QAAQ,CAACkB,QAAQ,CAACX,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIP,QAAQ,CAACa,QAAQ,EAAC;IACjC,CAAC;IACDM,OAAO,CAACP,EAAE,EAAE;MACVQ,OAAO,CAACC,GAAG,CAACT,EAAE;MACd,IAAIL,KAAI,GAAIP,QAAQ,CAACa,QAAQ,EAAE;MAC/BN,KAAI,GAAIA,K