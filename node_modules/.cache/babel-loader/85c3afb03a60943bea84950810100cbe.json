{"ast":null,"code":"// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    deleteT(id) {\n      console.log(id);\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  },\n  updated() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAWA;AACA,IAAIA,QAAO,GAAIC,OAAO,CAAC,yBAAyB;AAChD,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIH,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAE;MAC/B,IAAIC,QAAO,GAAIL,KAAK,CAACM,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACJ,EAAC,IAAKA,EAAE,CAAE;MACvDH,KAAK,CAACK,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACR,KAAK,CAACK,QAAQ,CAAC,CAACG,SAAQ;MACrDd,QAAQ,CAACe,QAAQ,CAACT,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAC;IACjC,CAAC;IACDM,OAAO,CAACP,EAAE,EAAE;MACVQ,OAAO,CAACC,GAAG,CAACT,EAAE;MACd,IAAIH,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAE;MAC/BJ,KAAI,GAAIA,KAAK,CAACa,MAAM,CAAEN,IAAI,IAAKA,IAAI,CAACJ,EAAC,KAAMA,EAAE;MAC7CT,QAAQ,CAACe,QAAQ,CAACT,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAC;IACjC;EACF,CAAC;EACDU,OAAO,GAAG;IACR,IAAI,CAACd,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAC;EACjC,CAAC;EACDW,OAAO,GAAG;IACR,IAAI,CAACf,KAAI,GAAIN,QAAQ,CAACU,QAAQ,EAAC;EACjC;AACF","names":["tasksObj","require","TaskTab","name","components","data","tasks","methods","u