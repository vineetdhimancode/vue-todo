{"ast":null,"code":"// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  methods: {\n    updateT(id) {\n      console.log(id);\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  },\n  updated() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAWA;AACA,IAAIA,QAAO,GAAIC,OAAO,CAAC,yBAAyB;AAChD,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACVC,OAAO,CAACC,GAAG,CAACF,EAAE;MACd,IAAIH,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAE;MAC/B,IAAIC,QAAO,GAAIP,KAAK,CAACQ,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACN,EAAC,IAAKA,EAAE,CAAE;MACvDH,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACV,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ;MACrDhB,QAAQ,CAACiB,QAAQ,CAACX,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;IACjC;EACF,CAAC;EACDM,OAAO,GAAG;IACR,IAAI,CAACZ,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;EACjC,CAAC;EACDO,OAAO,GAAG;IACR,IAAI,CAACb,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;EACjC;AACF","names":["tasksObj","require","TaskTab","name","components","data","tasks","methods","updateT","id","console","log","getTasks","objIndex","findIndex","task","completed","saveTask","mounted","updated"],"sourceRoot":"","sources":["/Users/vineetdhiman/Desktop/projects/vue/vue-todo/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Task list</h1>\n\n    <div v-for=\"task in tasks\" :key=\"task.title\">\n      <TaskTab :completed=\"task.completed\" :id=\"task.id\" :title=\"task.title\