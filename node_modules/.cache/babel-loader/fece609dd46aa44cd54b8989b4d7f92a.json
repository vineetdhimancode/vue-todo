{"ast":null,"code":"// @ is an alias to /src\n// let taskObj = require('../js/commonFunction.js')\nimport { saveTask, getTasks } from '../js/commonFunction.js';\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      saveTask(tasks);\n      this.tasks = getTasks();\n    },\n    deleteT(id) {\n      let tasks = getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      saveTask(tasks);\n      this.tasks = getTasks();\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = getTasks();\n  }\n};","map":{"version":3,"mappings":"AAeA;AACA;AACA,SAAQA,QAAQ,EAAEC,QAAQ,QAAO,yBAAwB;AAEzD,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIL,QAAQ,EAAE;MACtB,IAAIW,QAAO,GAAIN,KAAK,CAACO,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACH,EAAC,IAAKA,EAAE,CAAE;MACvDL,KAAK,CAACM,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACT,KAAK,CAACM,QAAQ,CAAC,CAACG,SAAQ;MACrDf,QAAQ,CAACM,KAAK;MACd,IAAI,CAACA,KAAI,GAAIL,QAAQ,EAAC;IACxB,CAAC;IACDe,OAAO,CAACL,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIL,QAAQ,EAAE;MACtBK,KAAI,GAAIA,KAAK,CAACW,MAAM,CAAEH,IAAI,IAAKA,IAAI,CAACH,EAAC,KAAMA,EAAE;MAC7CX,QAAQ,CAACM,KAAK;MACd,IAAI,CAACA,KAAI,GAAIL,QAAQ,EAAC;IACxB,CAAC;IACDiB,KAAK,CAACP,EAAE,EAAE;MACR,IAAI,CAACJ,MAAK,GAAI,IAAI;IACpB;EACF,CAAC;EACDY,OAAO,GAAG;IACR,IAAI,CAACb,KAAI,GAAIL,QAAQ,EAAC;EACxB;AACF","names":["saveTask","getTasks","TaskTab","name","components","data","tasks","isEdit","currentTask","methods","updateT","id","objIndex","findIndex","task","completed","deleteT","filter","editT","mounted"],"sourceRoot":"","sources":["/Users/vineetdhiman/Desktop/projects/vue/vue-todo/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Task list</h1>\n\n    <div v-if=\"!isEdit\">\n      <div v-for=\"task in tasks\" :key=\"task.title\">\n        <TaskTab :completed=\"task.completed\" :id=\"task.id\" :title=\"task.title\"\n        @updateTask=\"updateT\" @deleteTask=\"deleteT\" @editTask=\"editT\"/>\n      </div>\n    </div>\n    \n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n// let taskObj = require('../js/commonFunction.js')\nimport {saveTask, getTasks} from '../js/commonFunction.js'\n\nimport TaskTab from '../components/TaskTab.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab,\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    }\n  },\n  methods: {\n    updateT(id) {\n      let tasks = getTasks();\n      let objIndex = tasks.findIndex((task => task.id == id));\n      tasks[objIndex].completed = !tasks[objIndex].completed\n      saveTask(tasks)\n      this.tasks = getTasks()\n    },\n    deleteT(id) {\n      let tasks = getTasks();\n      tasks = tasks.filter((task) => task.id !== id)\n      saveTask(tasks)\n      this.tasks = getTasks()\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = getTasks()\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}