{"ast":null,"code":"// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    deleteT(id) {\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAeA;AACA,IAAIA,QAAO,GAAIC,OAAO,CAAC,yBAAyB;AAChD,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAE;MAC/B,IAAIC,QAAO,GAAIP,KAAK,CAACQ,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACJ,EAAC,IAAKA,EAAE,CAAE;MACvDL,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACV,KAAK,CAACO,QAAQ,CAAC,CAACG,SAAQ;MACrDhB,QAAQ,CAACiB,QAAQ,CAACX,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;IACjC,CAAC;IACDM,OAAO,CAACP,EAAE,EAAE;MACV,IAAIL,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAE;MAC/BN,KAAI,GAAIA,KAAK,CAACa,MAAM,CAAEJ,IAAI,IAAKA,IAAI,CAACJ,EAAC,KAAMA,EAAE;MAC7CX,QAAQ,CAACiB,QAAQ,CAACX,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;IACjC,CAAC;IACDQ,KAAK,CAACT,EAAE,EAAE;MACR,IAAI,CAACJ,MAAK,GAAI,IAAI;IACpB;EACF,CAAC;EACDc,OAAO,GAAG;IACR,IAAI,CAACf,KAAI,GAAIN,QAAQ,CAACY,QAAQ,EAAC;EACjC;AACF","names":["tasksObj","require","TaskTab","name","components","data","tasks","isEdit","currentTask","methods","updateT","id","getTasks","objIndex","findIndex","task","completed","saveTask","deleteT","filter","editT","mounted"],"sourceRoot":"","sources":["/Users/vineetdhiman/Desktop/projects/vue/vue-todo/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Task list</h1>\n\n    <div v-if=\"!isEdit\">\n      <div v-for=\"task in tasks\" :key=\"task.title\">\n        <TaskTab :completed=\"task.completed\" :id=\"task.id\" :title=\"task.title\"\n        @updateTask=\"updateT\" @deleteTask=\"deleteT\" @editTask=\"editT\"/>\n      </div>\n    </div>\n    \n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js')\nimport TaskTab from '../components/TaskTab.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab,\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false,\n      currentTask: null\n    }\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex((task => task.id == id));\n      tasks[objIndex].completed = !tasks[objIndex].completed\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    deleteT(id) {\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter((task) => task.id !== id)\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    editT(id) {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks()\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}