{"ast":null,"code":"// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js');\nimport TaskTab from '../components/TaskTab.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false\n    };\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex(task => task.id == id);\n      tasks[objIndex].completed = !tasks[objIndex].completed;\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    deleteT(id) {\n      console.log(id);\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter(task => task.id !== id);\n      tasksObj.saveTask(tasks);\n      this.tasks = tasksObj.getTasks();\n    },\n    editT() {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks();\n  },\n  updated() {\n    this.tasks = tasksObj.getTasks();\n  }\n};","map":{"version":3,"mappings":"AAiBA;AACA,IAAIA,QAAO,GAAIC,OAAO,CAAC,yBAAyB;AAChD,OAAOC,OAAM,MAAO,2BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACC,EAAE,EAAE;MACV,IAAIJ,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAE;MAC/B,IAAIC,QAAO,GAAIN,KAAK,CAACO,SAAS,CAAEC,IAAG,IAAKA,IAAI,CAACJ,EAAC,IAAKA,EAAE,CAAE;MACvDJ,KAAK,CAACM,QAAQ,CAAC,CAACG,SAAQ,GAAI,CAACT,KAAK,CAACM,QAAQ,CAAC,CAACG,SAAQ;MACrDf,QAAQ,CAACgB,QAAQ,CAACV,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAC;IACjC,CAAC;IACDM,OAAO,CAACP,EAAE,EAAE;MACVQ,OAAO,CAACC,GAAG,CAACT,EAAE;MACd,IAAIJ,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAE;MAC/BL,KAAI,GAAIA,KAAK,CAACc,MAAM,CAAEN,IAAI,IAAKA,IAAI,CAACJ,EAAC,KAAMA,EAAE;MAC7CV,QAAQ,CAACgB,QAAQ,CAACV,KAAK;MACvB,IAAI,CAACA,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAC;IACjC,CAAC;IACDU,KAAK,GAAG;MACN,IAAI,CAACd,MAAK,GAAI,IAAI;IACpB;EACF,CAAC;EACDe,OAAO,GAAG;IACR,IAAI,CAAChB,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAC;EACjC,CAAC;EACDY,OAAO,GAAG;IACR,IAAI,CAACjB,KAAI,GAAIN,QAAQ,CAACW,QAAQ,EAAC;EACjC;AACF","names":["tasksObj","require","TaskTab","name","components","data","tasks","isEdit","methods","updateT","id","getTasks","objIndex","findIndex","task","completed","saveTask","deleteT","console","log","filter","editT","mounted","updated"],"sourceRoot":"","sources":["/Users/vineetdhiman/Desktop/projects/vue/vue-todo/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Task list</h1>\n\n    <div v-if=\"!isEdit\">\n      <div v-for=\"task in tasks\" :key=\"task.title\">\n        <TaskTab :completed=\"task.completed\" :id=\"task.id\" :title=\"task.title\"\n        @updateTask=\"updateT\" @deleteTask=\"deleteT\" @editTask=\"editT\"/>\n      </div>\n    </div>\n    <div v-else>\n      <TaskForm></TaskForm>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nlet tasksObj = require('../js/commonFunction.js')\nimport TaskTab from '../components/TaskTab.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    TaskTab\n  },\n  data() {\n    return {\n      tasks: [],\n      isEdit: false\n    }\n  },\n  methods: {\n    updateT(id) {\n      let tasks = tasksObj.getTasks();\n      let objIndex = tasks.findIndex((task => task.id == id));\n      tasks[objIndex].completed = !tasks[objIndex].completed\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    deleteT(id) {\n      console.log(id)\n      let tasks = tasksObj.getTasks();\n      tasks = tasks.filter((task) => task.id !== id)\n      tasksObj.saveTask(tasks)\n      this.tasks = tasksObj.getTasks()\n    },\n    editT() {\n      this.isEdit = true;\n    }\n  },\n  mounted() {\n    this.tasks = tasksObj.getTasks()\n  },\n  updated() {\n    this.tasks = tasksObj.getTasks()\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}